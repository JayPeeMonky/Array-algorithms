package com.company;

public class Main {

    public static void main(String[] args) {
	    int [] numbers = {4, 55, -11, 43, 93};
	    final int key = 43;
	    boolean found = false;
		for (int i = 0; i < numbers.length; i++) {
			if (numbers[i] == key) {
				found = true;
				break;
			}
		}
		System.out.println(found);
    }
}
